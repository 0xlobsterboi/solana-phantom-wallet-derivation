{
  "name": "solana-phantom-wallet-derivation",
  "version": "1.0.0",
  "description": "programmatically derive solana wallets with mnemonic phrases compatible with the phantom wallet",
  "type": "module",
  "scripts": {
    "build": "esbuild src/*.ts --sourcemap --outdir=dist",
    "check": "tsc --noEmit",
    "test": "npm run build && node --experimental-specifier-resolution=node dist/example.js"
  },
  "keywords": [
    "solana",
    "phantom",
    "bip32",
    "bip39",
    "ec25519",
    "wallet"
  ],
  "author": "0xlobsterboi",
  "license": "ISC",
  "dependencies": {
    "@solana/web3.js": "^1.31.0",
    "bip39": "^3.0.4",
    "ed25519-hd-key": "^1.2.0"
  },
  "volta": {
    "node": "16.13.1"
  },
  "devDependencies": {
    "esbuild": "^0.14.3",
    "typescript": "^4.5.3"
  }
}
